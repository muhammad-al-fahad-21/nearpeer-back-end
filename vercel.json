{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/status",
        "dest": "./src/api/routes/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "./src/api/routes/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      }
    ]
  }
  
  
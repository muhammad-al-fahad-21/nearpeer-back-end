{
    "version": 2,
    "builds": [
      {
        "src": "src",
        "use": "@vercel/static",
        "config": {
          "outputDirectory": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/([^/]+)(.*)",
        "dest": "/src/api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ],
    "rewrites": [],
    "cleanUrls": true,
    "functions": {
      "api/**/*.js": {
        "maxDuration": 30
      }
    }
  }
  
  
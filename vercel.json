{
    "version": 2,
    "routes": [
      {
        "src": "/api/([^/]+)(.*)",
        "dest": "/src/api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/api/server.js"
      }
    ],
    "rewrites": [],
    "cleanUrls": true,
    "functions": {
      "api/**/*.js": {
        "maxDuration": 30
      }
    }
  }
  